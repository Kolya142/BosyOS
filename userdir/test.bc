U0 Print {
    eax 4;
    ecx esi;
    edx edi;
    ebx 1;
    INT 128;
}
U0 Demo {
    U32 count = 6;
    while count; {
        U32 c = 0x20434241;
        esi &c;
        edi 4;
        Print;
        count count - 1;
    }
}
U0 Main {
    U32 str = "Hello, world\n";
    U32 length = 13;
    esi str;
    edi length;
    Print;
    Dump;
}
U0 Reboot {
    eax 0xFE;
    OUT 0x64;
}
Uf Demo;
Main